            <header class="Navbar">

                <section class="flex">

                    <nav>

                        <img src="~/imgBia/Screenshot_2024-05-07_185200-removebg-preview.png" class="logo">

                        <a href="#home"style="text-decoration:none; color: #000">home</a>
                        <a href="#about"style="text-decoration: none; color: #000">about</a>
